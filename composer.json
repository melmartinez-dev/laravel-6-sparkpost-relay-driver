{
    "name": "melmartinez-dev/laravel-6-sparkpost-relay-driver",
    "description": "Sparkpost driver for laravel 7 and relay server",
    "keywords": [
        "mail",
        "laravel",
        "sparkpost"
    ],
    "homepage": "https://github.com/melmartinez-dev/laravel-6-sparkpost-relay-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Mel Martinez Parra",
            "email": "melmartinez.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^7.0",
        "swiftmailer/swiftmailer": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MMartinezDev\\SparkPostDriver\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MMartinezDev\\SparkPostDriver\\SparkPostDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
